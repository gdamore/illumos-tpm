set name=pkg.fmri value=pkg:/consolidation/tpm/messages@$(PKGVERS)
set name=pkg.description \
    value="Localizable messages for audio utilities consolidation"
set name=pkg.summary value="Localizable TPM messages"
set name=info.classification \
    value=oprg.opensolaris.category.2008:Development/System
set name=org.opensolaris.nodepend value=true
set name=variant.arch value=$(ARCH)
dir path=usr group=sys
dir path=usr/lib
dir path=usr/lib/locale
dir path=usr/lib/locale/C
dir path=usr/lib/locale/C/LC_MESSAGES
file path=usr/lib/locale/C/LC_MESSAGES/ILLUMOS_TPM.po group=sys
